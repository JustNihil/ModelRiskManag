    class ExportDashboardServlet extends HttpServlet {
        @Override
        protected void doGet(HttpServletRequest req, HttpServletResponse resp) throws IOException {
            addCorsHeaders(resp);
            try {
                System.out.println("Начинаем экспорт метрик...");
