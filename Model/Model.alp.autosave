                String[] risksCmd = {PYTHON_PATH, SUPERSET_CLI_PATH, "export_risks"};
                String risksJson = runPythonCommand(risksCmd);

                Gson gson = new Gson();
                Object metrics = gson.fromJson(metricsJson, Object.class);
                List<Risk> risksData = gson.fromJson(risksJson, new TypeToken<List<Risk>>(){}.getType());

                if (metrics instanceof java.util.Map && ((java.util.Map<?, ?>) metrics).containsKey("error")) {
                    throw new Exception("Ошибка экспорта метрик: " + ((java.util.Map<?, ?>) metrics).get("error"));
                }
                if (risksData == null || (risksData instanceof java.util.Map && ((java.util.Map<?, ?>) risksData).containsKey("error"))) {
                    throw new Exception("Ошибка экспорта рисков: " + (risksData == null ? "null" : ((java.util.Map<?, ?>) risksData).get("error")));
                }

                resp.setContentType("application/json");
                resp.getWriter().write(gson.toJson(new DashboardData(metrics, risksData, null))); // dashboardData = null
            } catch (Exception e) {
                System.err.println("Ошибка в ExportDashboardServlet: " + e.getMessage());
                e.printStackTrace();
                resp.sendError(HttpServletResponse.SC_INTERNAL_SERVER_ERROR, "Ошибка экспорта: " + e.getMessage());
            }
        }
    }
    
    class ProxyServlet extends HttpServlet {
        @Override
        protected void doPost(HttpServletRequest req, HttpServletResponse resp) throws IOException {
            addCorsHeaders(resp);
            String pathInfo = req.getPathInfo() != null ? req.getPathInfo() : "";
            String targetUrl = "http://localhost:8088/api" + pathInfo;
            System.out.println("Прокси: Обрабатываем POST запрос к " + targetUrl);

            try (CloseableHttpClient httpClient = HttpClients.createDefault()) {
                HttpPost proxyRequest = new HttpPost(targetUrl);

                String authHeader = req.getHeader("Authorization");
                if (authHeader != null) {
                    proxyRequest.setHeader("Authorization", authHeader);
                    System.out.println("Передаём заголовок Authorization: " + authHeader);
                }

                String requestBody = req.getReader().lines().collect(Collectors.joining());
                System.out.println("Тело запроса: " + requestBody);
                proxyRequest.setEntity(new StringEntity(requestBody, java.nio.charset.StandardCharsets.UTF_8));

                String contentType = req.getHeader("Content-Type");
                if (contentType != null) {
                    proxyRequest.setHeader("Content-Type", contentType);
                    System.out.println("Передаём Content-Type: " + contentType);
                } else {
                    proxyRequest.setHeader("Content-Type", "application/json");
                }

                try (CloseableHttpResponse proxyResponse = httpClient.execute(proxyRequest)) {
                    int statusCode = proxyResponse.getStatusLine().getStatusCode();
                    System.out.println("Ответ от целевого сервера: " + statusCode + " - " + proxyResponse.getStatusLine().getReasonPhrase());

                    resp.setStatus(statusCode);
                    Header contentTypeHeader = proxyResponse.getFirstHeader("Content-Type");
                    resp.setContentType(contentTypeHeader != null ? contentTypeHeader.getValue() : "application/json");

                    String responseBody = EntityUtils.toString(proxyResponse.getEntity());
                    System.out.println("Тело ответа от Superset: " + responseBody);
                    resp.getWriter().write(responseBody);
                }
            } catch (Exception e) {
                System.err.println("Ошибка прокси: " + e.getMessage());
                e.printStackTrace();
                resp.sendError(HttpServletResponse.SC_INTERNAL_SERVER_ERROR, "Ошибка прокси: " + e.getMessage());
            }
        }

        @Override
        protected void doGet(HttpServletRequest req, HttpServletResponse resp) throws IOException {
            addCorsHeaders(resp);
            String pathInfo = req.getPathInfo() != null ? req.getPathInfo() : "";
            String targetUrl = "http://localhost:8088/api" + pathInfo;
            System.out.println("Прокси: Обрабатываем GET запрос к " + targetUrl);

            try (CloseableHttpClient httpClient = HttpClients.createDefault()) {
                HttpGet proxyRequest = new HttpGet(targetUrl);

                String authHeader = req.getHeader("Authorization");
                if (authHeader != null) {
                    proxyRequest.setHeader("Authorization", authHeader);
                    System.out.println("Передаём заголовок Authorization: " + authHeader);
                }

                try (CloseableHttpResponse proxyResponse = httpClient.execute(proxyRequest)) {
                    int statusCode = proxyResponse.getStatusLine().getStatusCode();
                    System.out.println("Ответ от целевого сервера: " + statusCode + " - " + proxyResponse.getStatusLine().getReasonPhrase());

                    resp.setStatus(statusCode);
                    Header contentTypeHeader = proxyResponse.getFirstHeader("Content-Type");
                    resp.setContentType(contentTypeHeader != null ? contentTypeHeader.getValue() : "application/json");

                    String responseBody = EntityUtils.toString(proxyResponse.getEntity());
                    System.out.println("Тело ответа от Superset: " + responseBody);
                    resp.getWriter().write(responseBody);
                }
            } catch (Exception e) {
                System.err.println("Ошибка прокси: " + e.getMessage());
                e.printStackTrace();
                resp.sendError(HttpServletResponse.SC_INTERNAL_SERVER_ERROR, "Ошибка прокси: " + e.getMessage());
            }
        }

        @Override
        protected void doOptions(HttpServletRequest req, HttpServletResponse resp) throws IOException {
            addCorsHeaders(resp);
            resp.setStatus(HttpServletResponse.SC_OK);
        }
    }

    private void addCorsHeaders(HttpServletResponse resp) {
        resp.setHeader("Access-Control-Allow-Origin", "http://localhost:8000"); // Разрешаем только 8000
        resp.setHeader("Access-Control-Allow-Methods", "GET, POST, PUT, DELETE, OPTIONS");
        resp.setHeader("Access-Control-Allow-Headers", "Content-Type, Authorization, Accept, Accept-Language, Content-Language, X-Requested-With");
        resp.setHeader("Access-Control-Allow-Credentials", "true");
        resp.setHeader("Access-Control-Max-Age", "86400"); // Кэшируем preflight на 24 часа
    }

